#include <iostream>
#include<tuple>
#include <vector>
using namespace std;

int main(){
  vector<tuple<int, string, string>> people;
  tuple<int, string, string> person;
  while(cin >> person){
     cin >> person;
     people.push_back(person);
  }
  bool custom_comparison(const tuple<int, string, string> a, int, string, string> b){
      if (get<0>(a) != get<0>(b)) {
          return get<0>(a) > get<0>(b);
      }
      if (get<1>(a) != get<1>(b)) {
          return get<1>(a) < get<1>(b);
      }
      return get<2>(a) < get<2>(b);
  }
  sort(people.begin(), people.end(), custom_comparison);
  return people;
}
